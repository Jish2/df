{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "terminal.integrated.fontFamily": "'FiraCode Nerd Font', monospace",
  "editor.fontLigatures": true,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', 'FiraCode Nerd Font', monospace",
  "workbench.editor.pinnedTabSizing": "shrink",
  "console-ninja.featureSet": "Community",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "console-ninja.fontSize": 14,
  "editor.largeFileOptimizations": false,
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "github-enterprise.uri": "https://github.tesla.com/",
  "security.promptForLocalFileProtocolHandling": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "off",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[lua]": {
    "editor.defaultFormatter": "trixnz.vscode-lua"
  },
  "leetcode.workspaceFolder": "/Users/jgoon/.leetcode",
  "leetcode.defaultLanguage": "python3",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.hint.commentDescription": false,
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
  },
  "cSpell.userWords": [
    "allstatus",
    "blocknote",
    "capex",
    "Cybertruck",
    "Deburring",
    "dotenv",
    "Floorspace",
    "Genji",
    "GSMEFDE",
    "Hasura",
    "IEEPA",
    "jgoon",
    "miniview",
    "Pipeable",
    "popleft",
    "reusables",
    "sqlhelper",
    "Toggleable",
    "usehooks",
    "uuidv",
    "welldone",
    "worktree"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.activityBar.location": "top",
  "gitlens.remotes": [
    {
      "domain": "github.tesla.com",
      "type": "GitHub"
    }
  ],
  "github-actions.use-enterprise": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command"
  ],
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro", // Enable .astro
    "typescript", // Enable .ts
    "typescriptreact" // Enable .tsx
  ],
  "githubPullRequests.pullBranch": "never",
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path-mac": "/Users/jgoon/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path-mac": "/Users/jgoon/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vs-kubernetes.crd-code-completion": "disabled"
  },
  "terminal.integrated.env.osx": {},
  "[nix]": {
    "editor.defaultFormatter": "brettm12345.nixfmt-vscode"
  },
  "nixfmt.path": "/run/current-system/sw/bin/nixfmt",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "editor.lineNumbers": "relative",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "json": false,
    "typescriptreact": true,
    "typescript": true
  },
  "editor.multiCursorModifier": "alt",
  "workbench.colorTheme": "GitHub Dark Default",
  "zenMode.showTabs": "single",
  "window.customTitleBarVisibility": "windowed",
  "window.titleBarStyle": "custom",
  "github.copilot.nextEditSuggestions.enabled": true
}
